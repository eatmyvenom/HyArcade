<html lang="en">
  <head>
    {{{head}}}
    <link href="generic.css" rel="stylesheet" />
    <title>Arcade Leaderboard</title>
    <script src="generic.js"></script>
  </head>
  <body>
    <header>
      <p>
        <a href="."><span class="material-icons md-light">home</span> Home </a>
        <br />
        <a class="guildver" href="./guilds/"><span class="material-icons md-light">groups</span> Guilds </a>
      </p>
      <h1 class="minecraft">Generic</h1>

      <p id="info" class="minecraft">

      </p>
    </header>
    <address></address>

    <nav>
      <button onclick="btnClick(this)" id="lifetime" selected>Lifetime</button>
      <button onclick="btnClick(this)" id="day">Daily</button>
      <button onclick="btnClick(this)" id="weekly">Weekly</button>
      <button onclick="btnClick(this)" id="monthly">Monthly</button>
    </nav>
    <main></main>
    <aside>
      <span>Max length: </span><input oninput="maxValChange(this.value)" type="number" value="25" />
    </aside>
    <footer>
      {{{footer}}}
    </footer>
  </body>
</html>