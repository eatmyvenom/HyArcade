{
    "name": "hyarcade",
    "version": "2.4.5",
    "private": true,
    "description": "A full system for gathering and displaying peoples hypixel arcade games stats",
    "main": "index.js",
    "packageManager": "yarn@3.1.1",
    "author": "EatMyVenom",
    "license": "MPL-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/eatmyvenom/hyarcade.git"
    },
    "readme": "README.md",
    "maintainers": [
        "EatMyVenom"
    ],
    "scripts": {
        "build:worker": "cd src/Worker;wrangler publish;cd ../..",
        "bot:test": "node index.js bot test",
        "bot": "node spawn-bots.js",
        "commit": "cz",
        "lint": "eslint './{src,packages,systems}/**/*.{js,mjs}' --fix",
        "postinstall": "is-ci || husky install",
        "api": "node --max-old-space-size=6000 server.js",
        "update": "node --max-old-space-size=4000 . miniupdate"
    },
    "dependencies": {
        "@commitlint/config-angular": "^16.0.0",
        "@discoveryjs/json-ext": "^0.5.6",
        "canvas": "^2.9.0",
        "discord.js": "^13.6.0",
        "fs-extra": "^10.0.0",
        "hyarcade-config": "*",
        "hyarcade-logger": "*",
        "hyarcade-requests": "*",
        "hyarcade-structures": "*",
        "hyarcade-utils": "*",
        "javascript-time-ago": "^2.3.6",
        "mongodb": "^4.3.1",
        "node-fetch": "2",
        "nodejs": "^0.0.0",
        "stackblur-canvas": "^2.5.0",
        "timsort": "^0.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.14.8",
        "@babel/eslint-parser": "^7.14.7",
        "@babel/preset-env": "^7.14.8",
        "@commitlint/cli": "^16.1.0",
        "@types/node": "^17.0.14",
        "commitizen": "^4.2.4",
        "commitlint": "^16.1.0",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-jsdoc": "^37.7.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-unicorn": "^40.1.0",
        "husky": "^7.0.4",
        "is-ci": "^3.0.1",
        "json-ts": "^1.6.4",
        "prettier": "^2.5.1"
    },
    "engines": {
        "yarn": ">=1.4.0"
    },
    "workspaces": [
        "packages/*"
    ],
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
  }
}
